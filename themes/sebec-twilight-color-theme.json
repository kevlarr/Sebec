    "semanticTokenColors": {
        "class.typeHint": "#5C7EAE",
        "function.decorator": "#4C9DB8",
        "parameter": "#4C9DB8",
        "selfParameter": {
            "foreground": "#69ADC4",
            "fontStyle": "bold"
        },
        "class": "#B48EAE",
        "property.declaration": "#C2C0B7",
        "class.declaration": {
            "foreground": "#C2C0B7",
            "fontStyle": "bold"
        },
        "method.declaration": {
            "foreground": "#C2C0B7",
            "fontStyle": "bold"
        },
        "variable.readonly": {
            "foreground": "#C2C0B7",
            "fontStyle": "bold"
        }
    },
    "tokenColors": [
        {
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#405C82",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword.control",
                "keyword.operator",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#5C7EAE"
            }
        },
        {
            "scope": [
                "storage.type.class",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#5C7EAE",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "punctuation.definition.decorator",
            "settings": {
                "foreground": "#4C9DB8"
            }
        },
        {
            "scope": "variable.parameter.function-call",
            "settings": {
                "foreground": "#A3759C"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#5E9C87"
            }
        }
    ]
