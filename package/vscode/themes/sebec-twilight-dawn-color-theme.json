{
    "name": "Sebec Twilight (Dawn)",
    "type": "light",
    "semanticHighlighting": true,
    "colors": {
        "terminal.background": "#f2f1ee",
        "terminal.foreground": "#454d5f",
        "terminal.selectionBackground": "#7e97c8",
        "terminal.selectionForeground": "#1a1d23",
        "terminal.findMatchBackground": "#9f6f97",
        "terminalCursor.foreground": "#2b5664",
        "terminalCursor.background": "#f6f6f4",
        "terminal.ansiBlack": "#111318",
        "terminal.ansiBrightBlack": "#1e2229",
        "terminal.ansiBlue": "#2b3e64",
        "terminal.ansiBrightBlue": "#3d588f",
        "terminal.ansiCyan": "#2b5664",
        "terminal.ansiBrightCyan": "#3d7a8f",
        "terminal.ansiGreen": "#375b33",
        "terminal.ansiBrightGreen": "#4e8349",
        "terminal.ansiMagenta": "#84587c",
        "terminal.ansiBrightMagenta": "#9f6f97",
        "terminal.ansiRed": "#82353c",
        "terminal.ansiBrightRed": "#a7444c",
        "terminal.ansiYellow": "#b89856",
        "terminal.ansiBrightYellow": "#d1b171",
        "terminal.ansiWhite": "#d7d5cc",
        "terminal.ansiBrightWhite": "#edece8",
        "foreground": "#111318",
        "editor.foreground": "#111318",
        "editorLineNumber.foreground": "#c5c2b5",
        "sideBar.background": "#edece8",
        "panel.background": "#f2f1ee",
        "panel.border": "#f2f1ee",
        "editor.background": "#f6f6f4",
        "editorWidget.background": "#fbfaf9",
        "widget.shadow": "#5e6a823f"
    },
    "semanticTokenColors": {
        "property": "#9b5741",
        "property.declaration": "#9b5741",
        "class": "#3d588f",
        "function": "#3d588f",
        "method": "#3d588f",
        "class.declaration": {
            "foreground": "#3d588f",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "#3d588f",
            "fontStyle": "bold"
        },
        "method.declaration": {
            "foreground": "#3d588f",
            "fontStyle": "bold"
        }
    },
    "tokenColors": [
        {
            "scope": [
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.section.class.begin",
                "punctuation.section.function.begin"
            ],
            "settings": {
                "foreground": "#aaa692"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#375b33",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage.type.class",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#3d7a8f",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#3d7a8f"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#84587c"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#84587c"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#84587c",
                "fontStyle": "italic"
            }
        }
    ]
}