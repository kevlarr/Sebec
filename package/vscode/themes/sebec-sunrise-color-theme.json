{
    "name": "Sebec Sunrise",
    "type": "light",
    "semanticHighlighting": true,
    "colors": {
        "terminal.background": "#f1f0e9",
        "terminal.foreground": "#4c566a",
        "terminal.selectionBackground": "#849ec2",
        "terminal.selectionForeground": "#202632",
        "terminal.findMatchBackground": "#b67cac",
        "terminalCursor.foreground": "#346879",
        "terminalCursor.background": "#f7f6f2",
        "terminal.ansiBlack": "#131720",
        "terminal.ansiBrightBlack": "#272d3a",
        "terminal.ansiBlue": "#405c82",
        "terminal.ansiBrightBlue": "#4a6a96",
        "terminal.ansiCyan": "#346879",
        "terminal.ansiBrightCyan": "#4e90a6",
        "terminal.ansiGreen": "#627e4e",
        "terminal.ansiBrightGreen": "#759a5b",
        "terminal.ansiMagenta": "#815679",
        "terminal.ansiBrightMagenta": "#b67cac",
        "terminal.ansiRed": "#84333a",
        "terminal.ansiBrightRed": "#ad5259",
        "terminal.ansiYellow": "#b19359",
        "terminal.ansiBrightYellow": "#dac08b",
        "terminal.ansiWhite": "#d1cec2",
        "terminal.ansiBrightWhite": "#e8e6de",
        "foreground": "#131720",
        "editor.foreground": "#131720",
        "editorLineNumber.foreground": "#c0bdaf",
        "sideBar.background": "#e8e6de",
        "panel.background": "#f1f0e9",
        "panel.border": "#f1f0e9",
        "editor.background": "#f7f6f2",
        "editorWidget.background": "#fbfbf9",
        "widget.shadow": "#59657d3f"
    },
    "semanticTokenColors": {
        "property": "#ab6249",
        "property.declaration": "#ab6249",
        "class": "#4a6a96",
        "function": "#4a6a96",
        "method": "#4a6a96",
        "class.declaration": {
            "foreground": "#4a6a96",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "#4a6a96",
            "fontStyle": "bold"
        },
        "method.declaration": {
            "foreground": "#4a6a96",
            "fontStyle": "bold"
        }
    },
    "tokenColors": [
        {
            "scope": [
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.section.class.begin",
                "punctuation.section.function.begin"
            ],
            "settings": {
                "foreground": "#b1aea0"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#627e4e",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage.type.class",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#4e90a6",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#4e90a6"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#815679"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#815679"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#815679",
                "fontStyle": "italic"
            }
        }
    ]
}