    },
    "tokenColors": [
        {
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#405c82",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword.control",
                "keyword.operator",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#6988b5"
            }
        },
        {
            "scope": [
                "storage.type.class",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#6988b5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "punctuation.definition.decorator",
            "settings": {
                "foreground": "#437789"
            }
        },
        {
            "scope": "variable.parameter.function-call",
            "settings": {
                "foreground": "#a9709f"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#4e7e62"
            }
        }
    ]
