    "tokenColors": [
        {
            "scope": [
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.section.class.begin",
                "punctuation.section.function.begin"
            ],
            "settings": {
                "foreground": "#485265"
            }
        },
        {
            "scope": [
                "meta.function-call.arguments keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#485265"
            }
        },
        {
            "scope": [
                "storage.type.class",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#3d5f8f",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "other",
                "multiple"
            ],
            "settings": {
                "foreground": "#3d5f8f",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.flow",
            "settings": {
                "foreground": "#3d5f8f",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.import",
            "settings": {
                "foreground": "#3d5f8f",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.operator.logical",
            "settings": {
                "foreground": "#3d5f8f",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "variable.parameter.function-call",
            "settings": {
                "foreground": "#4976b6"
            }
        },
        {
            "scope": [
                "keyword.other.DML.sql",
                "keyword.other.DDL",
                "keyword.other.alias.sql",
                "keyword.other.sql"
            ],
            "settings": {
                "foreground": "#4976b6"
            }
        },
        {
            "scope": "entity.name.tag.yaml",
            "settings": {
                "foreground": "#6991c9"
            }
        },
        {
            "scope": "punctuation.definition.decorator",
            "settings": {
                "foreground": "#499bb6"
            }
        },
        {
            "scope": "meta.function-call.generic.python",
            "settings": {
                "foreground": "#8cc6d9"
            }
        },
        {
            "scope": "support.function.string.sql",
            "settings": {
                "foreground": "#8cc6d9"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#4e7e62",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant.other.table-name.sql",
            "settings": {
                "foreground": "#d28f79"
            }
        },
        {
            "scope": "punctuation.definition.string,",
            "settings": {
                "foreground": "#a9709f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#a9709f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#a9709f"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#a9709f"
            }
        }
    ]
